{
   "background": {
      "persistent": true,
      "scripts": [ "src/bg/background.js" ]
   },
   "content_scripts": [ {
      "js": [ "src/inject/inject.js", "js/jquery/jquery.min.js" ],
      "matches": [ "https://photos.shutterfly.com/*" ]
   } ],
   "default_locale": "en",
   "description": "Download multiple photos at a time with ShutterFly",
   "homepage_url": "http://redth.codes",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ShutterFly Selected Photo Downloader",
   "omnibox": {
      "keyword": "ShutterFly"
   },
   "page_action": {
      "default_icon": "icons/icon19.png",
      "default_title": "Download Selected Photos"
   },
   "permissions": [ "contentSettings", "downloads", "https://photos.shutterfly.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2",
   "web_accessible_resources": [ "src/inject/page.js", "src/inject/downloadselected.js" ]
}
